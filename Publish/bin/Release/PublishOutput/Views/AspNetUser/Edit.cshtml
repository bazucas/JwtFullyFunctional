@model  Models.AspNetUser
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers

<form id="frmcrud" asp-controller="AspNetUser" asp-action="Edit"  method="post" class="form-horizontal">
  <input asp-for=Id type="hidden" />
  <input asp-for=ConcurrencyStamp type="hidden" />
  <div asp-validation-summary=ModelOnly class="text-danger"></div>

  <div class="modal-body">

    <div class="form-group">
      <label asp-for=ApsNr class="control-label col-md-3"></label>
      <div class="col-md-9">
        <input asp-for=ApsNr asp-crud-dictionary=@ViewBag.CrudDictionary />
        <span asp-validation-for=ApsNr class="text-danger"></span>
      </div>
    </div>

    <div class="form-group">
      <label asp-for=FirstName class="control-label col-md-3"></label>
      <div class="col-md-9">
        <input asp-for=FirstName asp-crud-dictionary=@ViewBag.CrudDictionary />
        <span asp-validation-for=FirstName class="text-danger"></span>
      </div>
    </div>

    <div class="form-group">
      <label asp-for=LastName class="control-label col-md-3"></label>
      <div class="col-md-9">
        <input asp-for=LastName asp-crud-dictionary=@ViewBag.CrudDictionary />
        <span asp-validation-for=LastName class="text-danger"></span>
      </div>
    </div>

    <div class="form-group">
      <label asp-for=NickName class="control-label col-md-3"></label>
      <div class="col-md-9">
        <input asp-for=NickName asp-crud-dictionary=@ViewBag.CrudDictionary />
        <span asp-validation-for=NickName class="text-danger"></span>
      </div>
    </div>

    <div class="form-group">
      <label asp-for=Email class="control-label col-md-3"></label>
      <div class="col-md-9">
        <input asp-for=Email asp-crud-dictionary=@ViewBag.CrudDictionary />
        <span asp-validation-for=Email class="text-danger"></span>
      </div>
    </div>

    <div class="form-group">
      <label asp-for=PhoneNumber class="control-label col-md-3"></label>
      <div class="col-md-9">
        <input asp-for=PhoneNumber asp-crud-dictionary=@ViewBag.CrudDictionary />
        <span asp-validation-for=PhoneNumber class="text-danger"></span>
      </div>
    </div>

    <div class="form-group">
      <label asp-for=PhoneNumberMobile class="control-label col-md-3"></label>
      <div class="col-md-9">
        <input asp-for=PhoneNumberMobile asp-crud-dictionary=@ViewBag.CrudDictionary />
        <span asp-validation-for=PhoneNumberMobile class="text-danger"></span>
      </div>
    </div>

    <div class="form-group">
      <label asp-for=IPSCnr class="control-label col-md-3"></label>
      <div class="col-md-9">
        <input asp-for=IPSCnr asp-crud-dictionary=@ViewBag.CrudDictionary />
        <span asp-validation-for=IPSCnr class="text-danger"></span>
      </div>
    </div>

    <div class="form-group">
      <label asp-for=IsActive class="control-label col-md-3"></label>
      <div class="col-md-9">
        <input asp-for=IsActive asp-crud-dictionary=@ViewBag.CrudDictionary />
        <span asp-validation-for=IsActive class="text-danger"></span>
      </div>
    </div>

  </div>
</form>

